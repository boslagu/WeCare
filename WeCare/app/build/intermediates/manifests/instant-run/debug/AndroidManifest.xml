        <activity android:name="com.example.thea.wecare.Learning" />
        <activity android:name="com.example.thea.wecare.Herbal" />
        <activity android:name="com.example.thea.wecare.Monitoring" />
        <activity android:name="com.example.thea.wecare.BookMark" />
        <activity android:name="com.example.thea.wecare.TermofUse" />
        <activity android:name="com.example.thea.wecare.AboutUs" />
        <activity android:name="com.example.thea.wecare.SignUp" />
        <activity android:name="com.example.thea.wecare.HerbalInfo" />
        <activity android:name="com.example.thea.wecare.BookmarkItem" />
        <activity android:name="com.example.thea.wecare.DiseaseInfo" />
        <activity android:name="com.example.thea.wecare.ViewUserDisease" />

        <provider
            android:name="com.android.tools.fd.runtime.InstantRunContentProvider"
            android:authorities="com.example.thea.wecare.com.android.tools.fd.runtime.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>